<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>Narrator</title>
    <style type="text/css">
      body {
          margin: 0;
      }
      #mainDisplay {
          float: left;
          position: absolute;
          width: 60%;
          height: 100%;
      }
      #panel {
          margin-left: 60%;
      }
      #panel button {
          display: block;
      }
      #singleNodeAST {
          float: left;
          position: absolute;
          width: 100%;
          height: 90%;
      }
      #nodeAST1 {
          float: left;
          position: absolute;
          width: 100%;
          height: 50%;
      }
      #nodeAST2 {
          margin-top: 25%;
          float: left;
          position: absolute;
          width: 100%;
          height: 50%;
      }
      /* #debugPrint { */
      /*     margin-top: 50%; */
      /*     width: 100%; */
      /*     resize: none; */
      /* } */
    </style>
  </head>
  <body>
    <div id="mainDisplay"></div>

    <div id="panel">
      <!-- <button onclick="document.getElementById('file-input').click();">Open file</button> -->
      <input id="fileInput" type="file"/>
      <button type="button" id="deleteNode">Remove selected node</button>
      <button type="button" id="testButton">Test</button>
      <div id="singleNodeAST"></div>
      <div id="nodeAST1"></div>
      <div id="nodeAST2"></div>
      <!-- <textarea id="debugPrint" rows="10" wrap="on" readonly></textarea> -->
    </div>

    <script src="../node_modules/sigma/build/sigma.min.js"></script>
    <script src="../node_modules/cytoscape/dist/cytoscape.min.js"></script>
    <script src="../node_modules/dagre/dist/dagre.min.js"></script>
    <script src="narrator.bc.js"></script>
  </body>
</html>
